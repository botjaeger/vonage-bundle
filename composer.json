{
    "name": "botjaeger/nexmo-bundle",
    "description": "Integrates nexmo-php in symfony 3 & 4",
    "type": "symfony-bundle",
    "keywords": [
        "symfony",
        "bundle",
        "nexmo",
        "sms",
        "2fa"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jarn Gotostos",
            "email": "jarn.gotostos@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Botjaeger\\NexmoBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1",
        "nexmo/client": "^1.6",
        "symfony/config": "^3.4|^4.1",
        "symfony/dependency-injection": "^3.4|^4.1",
        "symfony/http-kernel": "^3.4|^4.1",
        "symfony/yaml": "^3.4|^4.1"
    },
    "require-dev": {
        "symfony/framework-bundle": "^3.4|^4.1",
        "phpunit/phpunit": "^7.4",
        "symfony/browser-kit": "^4.3",
        "symfony/css-selector": "^4.3",
        "symfony/phpunit-bridge": "^4.3"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    }
}
